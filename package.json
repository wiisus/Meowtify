{
  "name": "meowtify-web",
  "version": "1.0.0",
  "description": "Meowtify player using only Qobuz squid API",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "keywords": [
    "meowtify",
    "music"
  ],
  "author": "Wiisus",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "discord.js": "^14.0.0",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "electron": "^31.7.7",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "appId": "com.wiisus.meowtify",
    "productName": "Meowtify",
    "win": {
      "target": "nsis",
      "icon": "assets/logo.ico"
    },
    "files": [
      "**/*",
      "!config.example.json"
    ],
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
